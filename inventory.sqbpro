    m.id AS material_id,
    m.name AS material_name,
    IFNULL(SUM(p.quantity), 0) AS purchased_qty,
    IFNULL(SUM(i.actual_quantity), 0) AS inventory_qty,
    IFNULL(SUM(p.quantity), 0) - IFNULL(SUM(i.actual_quantity), 0)
        AS actual_consumed_qty
FROM materials m
LEFT JOIN purchases p
    ON m.id = p.material_id
    AND strftime('%Y-%m', p.purchase_date) = '2026-02'
LEFT JOIN inventories i
    ON m.id = i.material_id
    AND strftime('%Y-%m', i.inventory_date) = '2026-02'
GROUP BY
    m.id,
    m.name;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
